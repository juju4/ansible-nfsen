[Unit]
Description=NFSen
After=remote-fs.target network.target network-online.target syslog.target

[Service]
Type=forking
ExecStart={{nfsen_bindir}}/nfsen start
ExecStop={{nfsen_bindir}}/nfsen stop
PIDFile={{nfsen_piddir}}/nfsend.pid
;Restart=on-failure
;RestartSec=300s

[Install]
WantedBy=multi-user.target
